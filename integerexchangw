32bitèŒƒå›´

class Solution {
    public int reverse(int x) {
        int j =0;
        while (x!=0){
            int k=x%10;
            if(j>214748364||(j==214748364&&k>7)){
                return 0;
            }
            if(j<-214748364||(j==-214748364&&k<-8)){
                return 0;
            }
            x=x/10;
            j=j*10+k;
        }
        return j;

    }
}
